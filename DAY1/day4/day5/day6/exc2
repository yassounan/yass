def swap_listes(liste1 , liste2):
    print("Before swap : \n")
    print("liste1  ",liste1 )
    print("liste2  ",liste2 )
    print("\n\nAfter swap : \n")
    temp = liste1[0]
    liste1[0]=liste2[0]
    liste2[0]=temp

    temp = liste1[0]
    liste1[2]=liste2[2]
    liste2[2]=temp
    print("liste1 ",liste1)
    print("liste2 ",liste2)
liste1=[1,2,3]
liste2=[4,5,6]
swap_listes(liste1,liste2)

